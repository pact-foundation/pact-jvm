{
  "match": true,
  "comment": "Messages match with regex",
  "expected": {
    "contents": {
      "contentType": "application/json",
      "encoded": false,
      "content": {
        "alligator": {
          "name": "Mary",
          "feet": 4,
          "favouriteColours": [
            "red",
            "blue"
          ]
        }
      }
    },
    "matchingRules": {
      "content": {
        "$.alligator.name": {
          "matchers": [
            {
              "match": "regex",
              "regex": "\\w+"
            }
          ]
        },
        "$.alligator.favouriteColours[0]": {
          "matchers": [
            {
              "match": "regex",
              "regex": "red|blue"
            }
          ]
        },
        "$.alligator.favouriteColours[1]": {
          "matchers": [
            {
              "match": "regex",
              "regex": "red|blue"
            }
          ]
        }
      }
    }
  },
  "actual": {
    "contents": {
      "contentType": "application/json",
      "encoded": false,
      "content": {
        "alligator": {
          "feet": 4,
          "name": "Harry",
          "favouriteColours": [
            "blue",
            "red"
          ]
        }
      }
    }
  }
}
